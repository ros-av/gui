        }
    },
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.3.4",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "bloomfilter": "^0.0.18",
        "chokidar": "^2.1.2",
        "dayjs": "^1.8.9",
        "downloads-folder": "^1.0.1",
        "fast-glob": "^2.2.6",
        "graceful-fs": "^4.1.15",
        "jquery": "^3.3.1",
        "line-by-line": "^0.1.6",
        "material-components-web": "^1.0.0",
        "md5-file": "^4.0.0",
        "node-notifier": "^5.4.0",
        "node-persist": "^3.0.4",
        "normalize.css": "^8.0.1",
        "request": "^2.88.0",
        "request-progress": "^3.0.0",
