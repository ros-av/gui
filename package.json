{
    "name": "@rosav/gui",
    "productName": "ROS AV",
    "version": "0.0.0",
    "description": "The GUI for ROS AV.",
    "main": "src/index.js",
    "bin": {
        "rosave": "electron-forge start"
    },
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "eslint src --color"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ros-av/gui.git"
    },
    "keywords": [
        "av",
        "antivirus",
        "viruslist"
    ],
    "bugs": {
        "url": "https://github.com/ros-av/gui/issues"
    },
    "homepage": "https://ros-av.github.io",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {
                        "options": {
                            "name": "rosav",
                            "categories": [
                                "Development",
                                "Utility"
                            ],
                            "maintainer": "Richie Bendall",
                            "homepage": "https://richienb.github.io"
                        }
                    }
                },
                {
                    "name": "@electron-forge/maker-dmg",
                    "config": {
                        "name": "ROS AV",
                        "background": "./assets/dmg-background.png"
                    }
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {
                        "options": {
                            "name": "ROS AV",
                            "maintainer": "Richie Bendall",
                            "homepage": "https://richienb.github.io"
                        }
                    }
                },
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ROS AV"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "config": {}
                }
            ]
        }
    },
    "dependencies": {
        "@material/auto-init": "^3.1.0",
        "@material/icon-button": "^3.0.0",
        "@material/linear-progress": "^3.1.0",
        "@material/list": "^3.1.0",
        "@material/ripple": "^3.1.0",
        "@material/switch": "^3.1.0",
        "@material/tab": "^3.0.0",
        "@material/tab-bar": "^3.1.0",
        "@material/tab-indicator": "^3.0.0",
        "@material/textfield": "^3.1.0",
        "@material/top-app-bar": "^3.1.0",
        "@octokit/rest": "^16.28.7",
        "@sentry/electron": "^0.17.3",
        "auto-launch": "^5.0.5",
        "bloomfilter": "^0.0.18",
        "bluebird": "^3.5.5",
        "chokidar": "^3.0.2",
        "count-lines-in-file": "^1.1.0",
        "dayjs": "^1.8.15",
        "downloads-folder": "^1.0.1",
        "electron-squirrel-startup": "^1.0.0",
        "electron-store": "^4.0.0",
        "esm": "^3.2.25",
        "fast-glob": "^3.0.4",
        "firstline": "^2.0.2",
        "fs-extra": "^8.1.0",
        "graceful-fs": "^4.2.1",
        "jquery": "^3.4.1",
        "line-by-line": "^0.1.6",
        "lz-string": "^1.4.4",
        "lzjs": "^1.3.0",
        "material-components-web": "^3.1.0",
        "md5-file": "^4.0.0",
        "node-notifier": "^5.4.1",
        "normalize.css": "^8.0.1",
        "open-file-explorer": "^1.0.2",
        "request": "^2.88.0",
        "request-progress": "^3.0.0",
        "temp-dir": "^2.0.0",
        "update-electron-app": "^1.5.0",
        "vue": "^2.6.8"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.43",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "electron": "^6.0.1",
        "eslint": "^6.1.0",
        "eslint-config-google": "^0.13.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-vue": "^5.2.3"
    }
}
