{
    "name": "@rosav/gui",
    "description": "The GUI for ROS AV.",
    "version": "0.0.0",
    "main": "main.js",
    "bin": {
        "rosav-gui": "electron"
    },
    "scripts": {
        "start": "electron .",
        "build": "electron-builder"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ros-av/gui.git"
    },
    "build": {
